@model List<CatViewModel>

<h1>All Cats Info</h1>
<hr /> 
<table>
    <th>
        <td>Name</td>
        <td>Age</td>
        <td>Breed</td>
        <td>Image</td>
    </th>
@foreach  (var item in @Model)
    {
        
        <tr>
            <td>@item.Name</td>
            <td>@item.Age</td>
            <td>@item.Breed</td>
            <td><img src="@item.ImageURL" width="100px" /></td>
            <td>
        <a asp-controller="Cat" asp-action="Details" asp-route-id="@item.Id">Details</a>
        <a asp-controller="Cat" asp-action="Edit" asp-route-id="@item.Id">Edit</a>
        <a asp-controller="Cat" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
    }
</table>